{
	"extensions": [
		"markdown.extensions.tables",
		"pymdownx.magiclink",
		"pymdownx.betterem",
		"pymdownx.tilde",
		"pymdownx.emoji",
		"pymdownx.keys",
		"pymdownx.highlight",
		"pymdownx.tasklist",
		"pymdownx.superfences",
		"pymdownx.saneheaders",
		"pymdownx.details",
		"pymdownx.snippets",
		"md_in_html",
		"attr_list",
		"admonition",
		"mdserver.extensions.headinglink"
	],
	"extension_configs": {
		"pymdownx.tilde": {
			"subscript": false
		},
		"pymdownx.emoji": {
			"alt": "short",
			"options": {
				"attributes": {
					"align": "absmiddle",
					"height": "20px",
					"width": "20px"
				},
				"image_path": "https://raw.githubusercontent.com/twitter/twemoji/refs/heads/master/assets/72x72/",
				"non_standard_image_path": "https://github.githubassets.com/images/icons/emoji/"
			}
		}
	}
}
